{
  "name": "@matidiaz/styles",
  "version": "1.0.0",
  "description": "Cascading Style Sheets for the projects of Matidiaz",
  "main": "dist/matidiaz-styles.min.css",
  "scripts": {
    "start": "node-sass src/styles/index.scss dist/matidiaz-styles.css --importer=node_modules/node-sass-package-importer/dist/cli.js --watch",
    "build": "npm run build:css && npm run build:css:min",
    "build:css": "node-sass src/styles/index.scss dist/matidiaz-styles.css --importer=node_modules/node-sass-package-importer/dist/cli.js --sourceMap=true",
    "build:css:min": "node-sass src/styles/index.scss dist/matidiaz-styles.min.css --importer=node_modules/node-sass-package-importer/dist/cli.js --sourceMap=true --style=compressed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matidiaz00/styles_library.git"
  },
  "author": "Matias Diaz",
  "license": "ISC",
  "homepage": "https://github.com/matidiaz00/styles_library#readme",
  "dependencies": {
    "bootstrap-scss": "^4.6.1",
    "node-sass": "^7.0.1",
    "node-sass-package-importer": "^5.3.2"
  }
}
